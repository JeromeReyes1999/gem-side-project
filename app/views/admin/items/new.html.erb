<!--Page-->
<div class="container" style="margin-top: 100px; height: 150vh">
  <div class=" w-25" >
      <%= link_to "Back", admin_items_path, class: 'btn btn-primary form-control'%>
  </div>
  <%= form_for @item, url: admin_items_path(@item), method: :post do |f| %>
    <%= render partial: 'form', locals: {f: f, item: @item} %>
    <%= f.submit "Create", class: 'border btn btn-primary form-control' %>
  <% end %>
</div>